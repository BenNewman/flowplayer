    "bean": "^1.0.15",
    "brfs": "^2.0.1",
    "browserify": "^16.2.2",
    "browserify-derequire": "^0.9.4",
    "class-list": "~0.1.1",
    "computed-style": "^0.3.0",
    "es5-shim": "^4.1.1",
    "extend-object": "~1.0.0",
    "ie8": "0.8.1",
    "is-function": "~1.0.1",
    "mochify": "~5.8.1",
    "node-sass": "^4.9.3",
    "postcss-cli": "^6.0.0",
    "postcss-unopacity": "^2.0.0",
